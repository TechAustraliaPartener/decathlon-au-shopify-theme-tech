@keyframes bounce {
  0%   { transform: scale(1,1)    translateY(0); }
  20%  { transform: scale(1.1,.9) translateY(0); }
  30%  { transform: scale(.9,1.1) translateY(-15px);}
  50%  { transform: scale(1,1) translateY(0); }
  100% { transform: scale(1,1) translateY(0); }
}
@-webkit-keyframes bounce {
  0%   { transform: scale(1,1)    translateY(0); }
  20%  { transform: scale(1.1,.9) translateY(0); }
  30%  { transform: scale(.9,1.1) translateY(-15px);}
  50%  { transform: scale(1,1) translateY(0); }
  100% { transform: scale(1,1) translateY(0); }
}

@keyframes bounceToLeft {
  0%   { transform: scale(1,1)    translateX(0); }
  20%  { transform: scale(1.1,.9) translateX(0); }
  30%  { transform: scale(.9,1.1) translateX(-15px);}
  50%  { transform: scale(1,1) translateX(0); }
  100% { transform: scale(1,1) translateX(0); }
}

@-webkit-keyframes bounceToLeft {
  0%   { transform: scale(1,1)    translateX(0); }
  20%  { transform: scale(.9,1.1) translateX(0); }
  30%  { transform: scale(1.1,.9) translateX(-15px);}
  50%  { transform: scale(1,1) translateX(0); }
  100% { transform: scale(1,1) translateX(0); }
}

