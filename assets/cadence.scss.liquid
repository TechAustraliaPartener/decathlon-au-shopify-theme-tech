.ico {
    font-family: "Font Awesome" !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
.ico--star:before {
  content: "\f005";
}

@media (min-width:640px) {
.anchorList {
  display:flex;
  justify-content:space-evenly;
  align-items:center;
  margin-bottom:0;
}

.anchorList-item {
  padding:0;
  margin-bottom:0;
}

.anchorList-link {
  padding: 0 0.5em;
}
}