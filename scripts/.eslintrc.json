{
  "extends": "plugin:@cloudfour/recommended",
  "plugins": ["graphql"],
  "rules": {
    "graphql/template-strings": [
      "error",
      {
        "env": "literal",
        "tagName": "gql"
      }
    ]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "google": true,
    "moment": true
  }
}
