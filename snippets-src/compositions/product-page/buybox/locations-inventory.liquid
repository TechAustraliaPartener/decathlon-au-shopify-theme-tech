<div class="product-inventory-locations">
  <div class="de-u-padBottom06" data-collapse="inventoryLocations">
    <div class="de-u-flex de-ProductFulfillment-option">
      {% include 'assets-icons-store'
        with class: 'de-u-textBlack de-Icon de-u-textGrow6 de-u-spaceRight03' %}
      <p class="de-u-spaceNone de-u-textMedium">
        <span class="de-u-textBold">Free</span> returns
      </p>
    </div>
  </div>
  <div class="de-u-padBottom06 de-collapse" id="inventoryLocations">
    <div class="inventory-locations" id="inventoryLocs">
      
      <div class="locs-row">
        {% include 'assets-icons-store'
        with class: 'de-u-textBlack de-Icon de-u-textGrow6 de-u-spaceRight03' %}
        Favourite store: Tempe Change
      </div>

      {% raw %}
      <div class="locs-row">
        <div class="t-table">
          <div class="t-row">
            <div class="t-cell title" v-text="delivery.name"></div>
            <div class="t-cell ready" v-text="delivery.ready"></div>
            <div class="t-cell availability" v-bind:class="delivery.availability.class" v-text="delivery.availability.text"></div>
            <div class="t-cell hours" v-text="delivery.hours"></div>
          </div>
        </div>
      </div>
      {% endraw %}

      <div class="locs-row">
        {% include 'assets-icons-store'
        with class: 'de-u-textBlack de-Icon de-u-textGrow6 de-u-spaceRight03' %}

        Click and Collect locations
      </div>

      {% raw %}
      <div class="t-table">
        <div class="t-row" v-for="location in locations">
          <div class="t-cell title" v-text="location.name"></div>
          <div class="t-cell ready" v-text="location.ready"></div>
          <div class="t-cell availability" v-bind:class="location.availability.class" v-text="location.availability.text"></div>
          <div class="t-cell hours" v-text="location.hours"></div>
        </div>
      </div>
      {% endraw %}

    </div>
  </div>
</div>